#include<iostream>
using namespace std;

int main() {


	for (int i = 0; i < 18; i++)
		cout << i << " ";

	cout << endl;

	for (int j = 25; j < 45; j++)
		cout << j << " ";

	cout << endl;

	for (int k = 40; k <= 80; k += 4)
		cout << k << " ";

	cout << endl;

	for (int m = 12; m >= -6; m -=3)
		cout << m << " ";

	cout << endl;

}

